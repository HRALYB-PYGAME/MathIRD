{
    "name": "testbtn",
    "displayStates": [  
        {
            "condition": "x<10",
            "text": "X: {x}"
        },
        {
            "condition": "x<20",
            "text": "X': {x}"
        },
        {
            "text": "X'': {x}"
        }
    ],
    "position": { "row": 0, "col": 0 },
    "terms": [
        {
            "name": "term",
            "condition": "x>=0",
            "expressions": [
                "x+=min[1, x]",
                "x+=1",
                "y+=min[x*10, 0]"
            ]
        },
        {
            "name": "y divider",
            "condition": "x>=15",
            "expressions": [
                "y/=3"
            ]
        }
    ]
}